{
  "filters": {},
  "rules": {
    "preset-nekohack-inc": true,
    "preset-ja-technical-writing": {
      "sentence-length": {
        "max": 90
      },
      "max-comma": {
        "max": 8
      }
    },
    "ja-technical-writing/max-kanji-continuous-len": {
      max: 8
    },
    "ja-technical-writing/no-exclamation-question-mark": false,
    "spellcheck-tech-word": true,
    "prh": {
      "rulePaths": [
        "./prh.yml"
      ]
    }
  }
}